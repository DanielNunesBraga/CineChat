@model IEnumerable<CineChat.Models.ChatUsers>

@{
    ViewBag.Title = "ListLoged";
}

<h2 class="text-center">ListLoged</h2>

<table class="table">
    <tr>
        <th>Channel Name</th>
        <th>Category</th>
        <th>Exit</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.chat.title, "Details", new {id = item.chat.ID })
        </td>
        <td>
            @item.chat.category.description
        </td>
        <td>
            @Html.ActionLink("Channel Logout", "ChatLogout", new { id = item.chat.ID })
        </td>
    </tr>
}

</table>
